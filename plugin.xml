<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-sentimask" version="0.0.1" 
  xmlns="http://apache.org/cordova/ns/plugins/1.0" 
  xmlns:android="http://schemas.android.com/apk/res/android">
  <name>SentiMask</name>
  <js-module name="SentiMask" src="www/SentiMask.js">
    <clobbers target="cordova.plugins.SentiMask" />
  </js-module>
  <platform name="android">
    <config-file parent="/*" target="res/xml/config.xml">
      <feature name="SentiMask">
        <param name="android-package" value="cordova-plugin-sentimask.SentiMask" />
      </feature>
    </config-file>
    <config-file parent="/*" target="AndroidManifest.xml"></config-file>
    <source-file src="src/android/SentiMask.java" target-dir="src/cordova-plugin-sentimask/SentiMask" />
  </platform>
</plugin>